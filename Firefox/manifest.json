{
    "manifest_version": 2,

    "name": "CDA-AntyReklama",
    "description": "Wtyczka usuwająca reklamy z odtwarzacza CDA",
    "author": "Redssu",
    "version": "1.0.2",

    "browser_action" : {
        "default_icon" : {
            "256" : "icon.png"
        },
        "default_title": "Otwórz ustawienia",
        "default_popup": "entryPoint.html"
    },

    "icons" : {
        "256" : "icon.png"
    },

    "content_scripts" : [
        {
            "matches" : [ "*://*.cda.pl/*", "*://cda.pl/*" ],
            "js" : [ "CDA-AntyReklama.js" ],
            "all_frames": true
        }
    ],

    "permissions": [
        "storage"
    ]
}